#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,i=0;
    cin>>t;
    int array[t];
    for(i=0;i<t;i++)
    {
        cin>>array[i];
    }
    for(i=0;i<t;i++)
    {
        int n=array[i];
        int a=1;
        if(n%2==0)
        {
            for(a=1;a<=n/2;a++)
            {
                cout<<a<<" "<<a<<" ";
            }
        }
        else
        {
            if(n<27)
            {
                cout<<"-1";
            }
            else
            {
                cout<<"2 1 3 3 2 4 4 5 5 6 1 7 7 6 8 8 9 9 10 10 11 11 12 12 13 13 1 ";
                for(a=14;a<=n/2;a++)
                {
                    cout<<a<<" "<<a<<" ";
                }
            }
            
            
            
        }
        cout<<endl;

    }
    return 0;
}
