#include<bits/stdc++.h>
using namespace std;
int main()
{
   int n,minm=INT_MAX,j=0,answer;
        cin>>n;
        for(j=0;j<n;j++)
        {
            int x;
            cin>>x;
            x=abs(x);
            minm=min(minm,x);
        }
        answer=minm;
        cout<<answer<<endl;
    return 0;

}
