#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,i;
    cin>>t;
    vector<int> answer(t);
    for(i=0;i<t;i++)
    {
        int k;
        cin>>k;
        answer[i]=(k%2);
    }
    for(i=0;i<t;i++)
    {
        if(answer[i])
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
    return 0;


}
