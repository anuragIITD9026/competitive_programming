#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int i,countm=0,countc=0;
    for(i=0;i<n;i++)
    {
        int a,b;
        cin>>a>>b;
        if(a>b)
        {
            countm++;
        }
        else if(a<b)
        {
            countc++;
        }
    }
    if(countm>countc)
    {
        cout<<"Mishka";
    }
    else if(countm<countc)
    {
        cout<<"Chris";
    }
    else
    {
        cout<<"Friendship is magic!^^";
    }
    return 0;
}
