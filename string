#include<iostream>
#include<algorithm>
#include<map>
using namespace std;
int main()
{
    int t,i;
    cin>>t;
    map <int,int> m;
    for(i=0;i<t;i++)
    {
        string s;
        cin>>s;
        
        int j=0,count=0;
        for(j=0;s[j]!='\0';j++)
        {
            if(s[j]=='1')
            {
                count++;
            }
        }
        m[i]=count;

    }
    for(i=0;i<t;i++)
    {
        cout<<m[i]<<endl;
    }


}
