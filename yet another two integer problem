#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
   int i;
    int t;
    cin>>t;
    vector<int> answer(t);
    for(i=0;i<t;i++)
    {
     int a,b;
     cin>>a>>b;
     answer[i]=(abs(b-a))/10+(!((abs(b-a))%10==0));
    }
    for(i=0;i<t;i++)
    {
     cout<<answer[i]<<endl;
    }
    return 0;
}
