#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
   int i,j;
    int t;
    cin>>t;
    vector<int> answer(t,0);
    for(i=0;i<t;i++)
    {
        int n;
        cin>>n;
        if(n%4==0)
        {
            answer[i]=1;
        }
    }
    for(i=0;i<t;i++)
    {
       if(answer[i])
       {
        cout<<"Bob"<<endl;
       }
       else
       {
        cout<<"Alice"<<endl;
       }
    }
    return 0;
}
