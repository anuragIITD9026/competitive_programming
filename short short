#include<bits/stdc++.h>
#include<algorithm>
using namespace std;
int main()
{
    int t,i;
    cin>>t;
   vector<int> answer(t,0);
   for(i=0;i<t;i++)
   {
    string str;
    cin>>str;
    if(str[0]=='a'||str[1]=='b'||str[2]=='c')
    {
        answer[i]=1;
    }
   }
   for(i=0;i<t;i++)
   {
    if(answer[i])
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }
   }
   return 0;
}
