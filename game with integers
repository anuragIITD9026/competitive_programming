#include<bits/stdc++.h>
using namespace std;
#define ll long long;
int main()
{
    int t;
    int i;
    cin>>t;
   vector<int> array(t);
   for(i=0;i<t;i++)
   {
    cin>>array[i];
   }
   for(i=0;i<t;i++)
   {
    if(array[i]%3==0)
    {
        cout<<"Second"<<endl;
    }
    else
    {
        cout<<"First"<<endl;
    }
   }
   return 0;
}
