class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int i=0,j=0;
        for(i=0;i<nums.size();i++)
        {
            for(j=0;j<i;j++)
            {
                if(nums[i]==target-nums[j])
                {
                   
                    return {i,j};
                }
            }

        }
      
        return {};
    }
};
